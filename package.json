{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "dev": "next dev -p 3004",
    "build": "next build",
    "start": "next start",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "migrate:undo:all": "node_modules/.bin/sequelize db:migrate:undo:all",
    "lint": "eslint ./src/** -c .eslintrc.json --ext js,jsx",
    "lint-fix": "eslint ./src/** -c .eslintrc.json --fix --ext js,jsx",
    "drop:db": "cross-env NODE_ENV=development node_modules/.bin/sequelize db:drop wrace",
    "create:db": "cross-env NODE_ENV=development node_modules/.bin/sequelize db:create wrace"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0-rc.1",
    "@mui/utils": "^5.0.0",
    "bcrypt": "^5.0.1",
    "clsx": "^1.1.1",
    "cookie": "^0.4.1",
    "formik": "^2.2.9",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "next": "11.1.0",
    "next-connect": "^0.10.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.0.3",
    "recharts": "^2.1.4",
    "sequelize": "^5.22.3",
    "use-sound": "^4.0.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-do-expressions": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^2.0.13",
    "sequelize-cli": "^6.2.0"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock",
      "<rootDir>/window.setup.js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "modulePaths": [
      "src/components",
      "__tests__"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "<rootDir>/src/**/*.jsx",
      "!<rootDir>/coverage/**/*.*",
      "!<rootDir>/build/**/*.*",
      "!<rootDir>/cypress/**/*.*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|JPG|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "engines": {
    "node": "16.4.2"
  }
}
